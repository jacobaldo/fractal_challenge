{
  "name": "fractal_challenge",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "setDev": "ENVFILE=.env.dev",
    "setQa": "ENVFILE=.env.qa",
    "setProd": "ENVFILE=.env",
    "ios:dev": "yarn setDev react-native run-ios --mode=Debug --scheme \"fractal_challenge_dev\" ",
    "ios:qa": "yarn setQa react-native run-ios --mode=Debug --scheme \"fractal_challenge_qa\"",
    "ios:prod": "yarn setProd react-native run-ios --mode=Debug --scheme \"fractal_challenge\"",
    "android:dev": "yarn setDev  react-native run-android",
    "android:prod": "yarn setProd  react-native run-android",
    "android:qa": "yarn setQa  react-native run-android",
    "android:prod-release": "cd android && ./gradlew assembleRelease",
    "android:prod-release-bundle": "cd android && ./gradlew bundleRelease",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^2.1.0",
    "@reduxjs/toolkit": "^2.5.1",
    "@types/react-native-dotenv": "^0.2.2",
    "axios": "^1.7.9",
    "react": "18.3.1",
    "react-native": "0.77.0",
    "react-native-config": "^1.5.3",
    "react-native-dotenv": "^3.4.11",
    "react-redux": "^9.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.3",
    "@babel/runtime": "^7.25.0",
    "@react-native-community/cli": "15.0.1",
    "@react-native-community/cli-platform-android": "15.0.1",
    "@react-native-community/cli-platform-ios": "15.0.1",
    "@react-native/babel-preset": "0.77.0",
    "@react-native/eslint-config": "0.77.0",
    "@react-native/metro-config": "0.77.0",
    "@react-native/typescript-config": "0.77.0",
    "@testing-library/react-native": "^13.0.1",
    "@types/jest": "^29.5.13",
    "@types/react": "^18.2.6",
    "@types/react-test-renderer": "^18.0.0",
    "@types/redux-mock-store": "^1.5.0",
    "eslint": "^8.19.0",
    "jest": "^29.6.3",
    "prettier": "2.8.8",
    "react-test-renderer": "18.3.1",
    "redux-mock-store": "^1.5.5",
    "typescript": "5.0.4"
  },
  "engines": {
    "node": ">=18"
  }
}
